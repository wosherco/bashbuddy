{
  "name": "@bashbuddy/agent",
  "version": "0.1.3",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "default": "./src/index.ts"
    },
    "./legacy": {
      "types": "./dist/src/legacy/index.d.ts",
      "default": "./src/legacy/index.ts"
    },
    "./transport": {
      "types": "./dist/src/transport/index.d.ts",
      "default": "./src/transport/index.ts"
    },
    "./tools": {
      "types": "./dist/src/tools/index.d.ts",
      "default": "./src/tools/index.ts"
    },
    "./env": "./env.ts"
  },
  "license": "Apache-2.0",
  "scripts": {
    "build": "tsc",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "tsc --watch",
    "format": "prettier --check . --ignore-path ../../.gitignore --ignore-path ../../.prettierignore --ignore-path .prettierignore --ignore-path .gitignore ",
    "lint": "eslint",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@bashbuddy/validators": "workspace:*",
    "@bashbuddy/common": "workspace:*",
    "@t3-oss/env-core": "catalog:",
    "@langchain/core": "catalog:",
    "@langchain/langgraph": "catalog:",
    "langfuse": "^3.35.2",
    "langfuse-langchain": "^3.35.2",
    "zod": "catalog:",
    "zod-to-json-schema": "^3.23.0"
  },
  "devDependencies": {
    "@bashbuddy/eslint-config": "workspace:*",
    "@bashbuddy/prettier-config": "workspace:*",
    "@bashbuddy/tsconfig": "workspace:*",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:"
  },
  "prettier": "@bashbuddy/prettier-config"
}
